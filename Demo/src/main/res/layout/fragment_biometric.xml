<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tip_view"
        style="@style/LibTitleTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="--" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/biometric_view"
        style="@style/LibTitleTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="生物指纹API测试" />

    <com.angcyo.uicore.component.FingerPrinterView
        android:id="@+id/biometric_printer_view"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="@dimen/lib_xhdpi" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/biometric_view2"
        style="@style/LibTitleTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="生物指纹API测试2" />

    <com.angcyo.uicore.component.FingerPrinterView
        android:id="@+id/biometric_printer_view2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="@dimen/lib_xhdpi" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/finger_view"
        style="@style/LibTitleTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="旧指纹API测试" />

    <com.angcyo.uicore.component.FingerPrinterView
        android:id="@+id/finger_printer_view"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="@dimen/lib_xhdpi" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/finger_view2"
        style="@style/LibTitleTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="指纹API(my)" />

    <com.angcyo.uicore.component.FingerPrinterView
        android:id="@+id/finger_printer_view2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="@dimen/lib_xhdpi" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/result_view"
        style="@style/LibTitleTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        tools:text="--" />

</LinearLayout>